<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.webadmin.mapper.EmployeeMapper">
    <select id="selectEmployeeById" resultType="com.webadmin.bean.Employee">
        select * from tb_employee where id = #{id}
    </select>
</mapper>